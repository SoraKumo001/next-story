{
  "name": "next-story",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "yarn redis && ts-node -P server/tsconfig.json server/index.ts",
    "build": "next build",
    "start": "cross-env NODE_ENV=production ts-node -P server/tsconfig.json server/index.ts",
    "start-docker": "docker-compose -f docker/docker-compose.yml up -d --build",
    "export": "next build && next export",
    "redis": "docker-compose -f docker/docker-compose.yml up -d --build redis",
    "storybook": "start-storybook",
    "reg-local": "node .github/scripts/reg-local",
    "capture": "npm-run-all -p -r sc:*",
    "sc:start-storybook": "start-storybook -p 9001 --ci",
    "sc:screenshot": "storycap http://localhost:9001 --stateChangeDelay 3000 --serverCmd \"yarn start-storybook -p 9001 --ci\"",
    "graphql-gen": "graphql-codegen --config graphql/codegen.js"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@graphql-codegen/cli": "^1.12.2",
    "@graphql-codegen/introspection": "^1.12.2",
    "@graphql-codegen/typescript": "^1.12.2",
    "@graphql-codegen/typescript-operations": "^1.12.2",
    "@graphql-codegen/typescript-react-apollo": "^1.12.2",
    "@storybook/react": "^5.3.14",
    "@types/connect-redis": "^0.0.13",
    "@types/express": "^4.17.3",
    "@types/express-session": "^1.17.0",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@types/redis": "^2.8.16",
    "@types/request": "^2.48.4",
    "apollo-boost": "^0.4.7",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.1",
    "connect-redis": "^4.0.4",
    "cross-env": "^7.0.2",
    "express-session": "^1.17.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.2.2",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.13.0",
    "redis": "^3.0.2",
    "reg-cli": "^0.16.0",
    "storycap": "^2.3.5",
    "ts-node": "^8.6.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.8.3"
  }
}
